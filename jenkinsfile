pipeline {
   agent {
      label 'papu'
   }
   tools {
     maven "maven"  
   }
   stages {
      stage('Git Checkout') {
         steps {
            git 'https://github.com/localmain/loginapp.git'   
         }
      }

    stage ('Maven Goal'){
      steps {
         sh "mvn clean install package    " 
      }    
    }
   
    }
}
